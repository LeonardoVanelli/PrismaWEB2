<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="Leonardo Vanelli">

    <link rel="shortcut icon" href="~/generico2.png" type="image/x-icon" />
    <title>@ViewBag.Title - My Genric Application</title>
    <!-- CSS -->
    @Styles.Render("~/bundles/css")
    <link href="~/Content/fontawesome-free-5.4.1-web/css/all.min.css" rel="stylesheet" />
    <link href="~/Content/font-awesome2/css/font-awesome.min.css" rel="stylesheet" />
</head>

<body class="fixed-nav sticky-footer bg-dark" id="page-top">
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark fixed-top" id="mainNav">
        <a class="navbar-brand" href="~/Home">
            <img src="~/generico2.png" width="30" height="30" class="d-inline-block align-top" alt="">
            Genérico
        </a>
        <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <!-- Menus -->
        <div class="collapse navbar-collapse" id="navbarResponsive">
            @{
                Html.RenderPartial("~/Views/Shared/Menus/Superior.cshtml");
                Html.RenderPartial("~/Views/Shared/Menus/Lateral.cshtml");
                Html.RenderPartial("~/Views/Shared/Menus/SuperiorDireito.cshtml");
            }
        </div>
    </nav>
    <div class="content-wrapper">
        <div class="container-fluid">
            <!-- Breadcrumbs-->
            <ol class="breadcrumb">
                <li class="breadcrumb-item">
                    @Html.MvcSiteMap().SiteMapPath()
                </li>
            </ol>
            <div class="row">
                <div class="col-12">
                    @RenderBody()
                    <!--Menu cadastro rapido-->
                    <div id="itens-cadastro-rapido" class="dropdown-menu">
                        <a class="dropdown-item" href="~/Pessoas/Create">Pessoa</a>
                        <a class="dropdown-item" href="~/Leis/Create">Leis</a>
                        <a class="dropdown-item" href="#">Something else here</a>
                    </div>
                </div>
            </div>
        </div>
        <!-- /.container-fluid-->
        <!-- /.content-wrapper-->
        <footer class="sticky-footer">
            <div class="container">
                <div class="text-center">
                    <small>Copyright © PrismaWEB 2018</small>
                </div>
            </div>
        </footer>
        <!-- Scroll to Top Button-->
        <a class="scroll-to-top rounded" href="#page-top">
            <i class="fa fa-angle-up"></i>
        </a>
        @{
            Html.RenderPartial("~/Views/Shared/Modals/Logout.cshtml");
            Html.RenderPartial("~/Views/Shared/Modals/Deletar.cshtml");
        }
    </div>

</body>
</html>
<!-- JAVASCRIPT -->
@Scripts.Render("~/bundles/jquery")
@Scripts.Render("~/bundles/bootstrap")
@Scripts.Render("~/bundles/charts")
@Scripts.Render("~/bundles/datatables")
@Scripts.Render("~/bundles/js")
@Scripts.Render("~/bundles/inputmask")

<script>    
    function gettoken() {
        var token = '@Html.AntiForgeryToken()';


        token = $(token).val();
        return token;
    }
</script>